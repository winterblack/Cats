<p id="notice"><%= notice %></p>

<h1>Listing Cat Rental Requests</h1>

<table>
  <thead>
    <tr>
      <th>Cat</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cat_rental_requests.each do |cat_rental_request| %>
      <tr>
        <td><%= cat_rental_request.cat_id %></td>
        <td><%= cat_rental_request.start_date %></td>
        <td><%= cat_rental_request.end_date %></td>
        <td><%= cat_rental_request.status %></td>
        <td><%= link_to 'Show', cat_rental_request %></td>
        <td><%= link_to 'Edit', edit_cat_rental_request_path(cat_rental_request) %></td>
        <td><%= link_to 'Destroy', cat_rental_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cat rental request', new_cat_rental_request_path %>
